<config>
    
    <acl>
        <resources>
            <admin>
                <children>
                    <system>
                        <children>
                            <config>
                                <children>
                                    <videoupload translate="title" module="videoupload">
                                        <title>Video Upload</title>
                                    </videoupload>
                                </children>
                            </config>
                        </children>
                    </system>
                    
                </children>
            </admin>
        </resources>
    </acl>
</config>
