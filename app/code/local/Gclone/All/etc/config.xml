<?xml version="1.0"?>
<config>
	<modules>
		<Gclone_All>
			<version>0.1.0</version>
		</Gclone_All>
	</modules>
	<frontend>
		<routers>
			<all>
				<use>standard</use>
				<args>
					<module>Gclone_All</module>
					<frontName>all</frontName>
				</args>
			</all>
		</routers>
		<layout>
			<updates>
				<all>
					<file>all.xml</file>
				</all>
			</updates>
		</layout>
	</frontend>
	<admin>
		<routers>
			<all>
				<use>admin</use>
				<args>
					<module>Gclone_All</module>
					<frontName>all</frontName>
				</args>
			</all>
		</routers>
	</admin>
	<global>
		<models>
			<all>
				<class>Gclone_All_Model</class>
				<resourceModel>all_mysql4</resourceModel>
			</all>
			<all_mysql4>
				<class>Gclone_All_Model_Mysql4</class>
				<entities>
					<all>
						<table>all</table>
					</all>
				</entities>
			</all_mysql4>
		</models>
		<resources>
			<all_setup>
				<setup>
					<module>Gclone_All</module>
				</setup>
				<connection>
					<use>core_setup</use>
				</connection>
			</all_setup>
			<all_write>
				<connection>
					<use>core_write</use>
				</connection>
			</all_write>
			<all_read>
				<connection>
					<use>core_read</use>
				</connection>
			</all_read>
		</resources>
		<blocks>
			<all>
				<class>Gclone_All_Block</class>
			</all>
		</blocks>
		<helpers>
			<all>
				<class>Gclone_All_Helper</class>
			</all>
		</helpers>
	</global>
</config>